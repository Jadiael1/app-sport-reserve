{
  "expo": {
    "name": "SportReserve",
    "slug": "SportReserve",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "scheme": "SportReserve",
    "platforms": ["ios", "android", "web"],
    "splash": {
      // "image": "./assets/ball.json",
      "resizeMode": "contain",
      "backgroundColor": "#E8F4F8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.sportreserve"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#E8F4F8"
      },
      "package": "com.sportreserve"
    },
    "web": {
      "logo.png": "./assets/logo.png",
      "bundle": "metro"
    },
    "plugins": ["expo-router"]
  }
}
